<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ColorTextButton">
        <attr name="selectedTextColor" format="color"></attr>
        <attr name="unselectedTextColor" format="color"></attr>
    </declare-styleable>

    <declare-styleable name="SlideSwitch">
        <attr name="themeColor" format="color" />
        <attr name="isOpen" format="boolean" />
        <attr name="shape">
            <enum name="rect" value="1" />
            <enum name="circle" value="2" />
        </attr>
    </declare-styleable>
    private int focusWidth;
    private int colorFocusedText;
    private int colorUnfocusedText;
    private int colorFocus;

    <declare-styleable name="StoreTabBar">
        <attr name="textSize" format="float" />
        <attr name="focusWidth" format="dimension" />
        <attr name="focusedTextColor" format="color" />
        <attr name="unfocusedTextColor" format="color" />
        <attr name="focusedResource" format="integer" />
    </declare-styleable>

</resources>