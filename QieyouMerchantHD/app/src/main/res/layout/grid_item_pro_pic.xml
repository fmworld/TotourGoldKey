<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fresco="http://schemas.android.com/apk/res-auto"
    android:layout_width="93dp" android:layout_height="93dp"
    android:orientation="vertical"

    >
    <com.facebook.drawee.view.SimpleDraweeView
        android:id="@+id/home_mgr_pro_pic"
        android:layout_width="93dp"
        android:layout_height="93dp"
        fresco:placeholderImage="@drawable/pro_default_post"
        fresco:placeholderImageScaleType="centerCrop"
        />
    <ImageView
        android:id="@+id/home_mgr_pro_pic_delete_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="3dp"
        android:src="@drawable/home_mgr_pro_pic_delete_icon"
        />
</RelativeLayout>
